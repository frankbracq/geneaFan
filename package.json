    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-search-and-replace": "^1.0.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
