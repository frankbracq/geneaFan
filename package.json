    "@uppy/aws-s3": "^4.1.0",
    "@uppy/core": "^4.2.0",
    "blob-stream": "^0.1.3",
    "bootstrap": "^5.2.0",
    "bootstrap5-toggle": "^5.0.4",
    "core-js": "^3.35.1",
    "d3": "^7.9.0",
    "driver.js": "^1.3.5",
    "fast-json-patch": "^3.1.1",
    "jquery": "^3.5.1",
    "jsdom": "^24.1.0",
    "jsonpointer": "^5.0.1",
    "lodash": "^4.17.21",
    "mobx": "^6.13.0",
    "os-browserify": "^0.3.0",
    "panzoom": "^9.4.3",
    "parse-gedcom": "^1.0.5",
    "path-browserify": "^1.0.1",
    "pdfkit": "^0.15.0",
    "popper.js": "^1.16.1",
    "read-gedcom": "^0.3.2",
    "screenfull": "^6.0.2",
    "svg-to-pdfkit": "^0.1.8",
    "tinycolor2": "^1.6.0",
    "tom-select": "^2.3.1",
